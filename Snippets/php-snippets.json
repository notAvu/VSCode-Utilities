	"Private class attribute":{
		"prefix" : "privGetSet",
		"body": [
					"private $$1;",
					"function get$1(){",
					"return $$this->$1;",
					"}",
					"function set$1($$1){",
					"$$this->$1 = $$1;",
					"}",
				],
				"description": "Private class attribute with getter and setter"
	}
}
